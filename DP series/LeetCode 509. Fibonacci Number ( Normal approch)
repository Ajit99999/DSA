/**
 * @param {number} n
 * @return {number}
 */
var fib = function(n) {
    
    
    function find(n)
    {
        if(n === 0 || n === 1 )
            {
                return n
            }
        else
            {
               return find(n-1) + find(n-2) 
            }
    }
    
    let ans = find(n)
    return ans
};
