/**
 * @param {number} n
 * @return {number}
 */
var fib = function(n) {
    
    
    function find(n)
    {
       if(n === 0 || n === 1 || n === 2)
        {
        return n
        }
      else 
       {
       return fib(n-1) + fib(n-2)
       }
    }
    
    let ans = find(n)
    return ans
};
