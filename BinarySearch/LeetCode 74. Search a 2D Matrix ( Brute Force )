/**
 * @param {number[][]} matrix
 * @param {number} target
 * @return {boolean}
 */
var searchMatrix = function(matrix, target) {
    
    let ans = false;
    for(let i = 0 ; i<= matrix.length - 1 ; i++)
        {
            for(let j = 0 ; j<= matrix[i].length - 1 ; j++ )
                {
                    if(matrix[i][j] === target)
                        {
                            ans = true
                            break
                        }
                }
        }
    return ans
};
