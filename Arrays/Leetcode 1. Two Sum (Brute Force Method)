/**
 * @param {number[]} nums
 * @param {number} target
 * @return {number[]}
 */
var twoSum = function(nums, target) {
    let ans_arry = []
    for(let i = 0 ; i<=nums.length - 1; i++)
        {
            for(let j = i+1 ; j<= nums.length - 1; j++)
                {
                    if(nums[i] + nums[j] === target)
                        {
                            ans_arry.push(i);
                            ans_arry.push(j);
                            break;
                        }
                }
        }
    return ans_arry
};
