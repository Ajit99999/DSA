/**
 * @param {number[]} nums
 * @return {number[]}
 */
var runningSum = function(nums) {
    let sum = 0;
    let res = [];
    for(let i = 0 ; i<= nums.length - 1 ; i++)
        {
            sum = 0;
            for(let j = 0 ; j <= i; j++ )
                {
                    sum+= nums[j]        
                }
            res.push(sum);
            
        }
    return res
};
