/**
 * @param {number[]} nums
 * @return {number}
 */
var missingNumber = function(nums) {
    
    let sum = Math.floor((nums.length * (nums.length + 1))/2);
    let additionsum = 0;
    for(let i = 0 ; i <= nums.length - 1 ; i++)
    {
        additionsum += nums[i]
    }
    return sum-additionsum
};
